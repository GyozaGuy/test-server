<style>
  * {
    box-sizing: border-box;
  }

  body {
    font-family: Aurebesh, sans-serif;
    font-size: 80px;
    font-weight: bold;
    position: relative;
    text-transform: uppercase;
  }

  #outer {
    align-items: center;
    background-color: #18366c;
    border: 10px solid #a9beeb;
    border-radius: 50%;
    display: flex;
    height: 832px;
    justify-content: center;
    margin: auto;
    width: 832px;
  }

  #inner {
    border: 10px solid #a9beeb;
    border-radius: 50%;
    height: 640px;
    width: 640px;
  }

  svg, img {
    position: absolute;
  }

  svg {
    top: 0;
    z-index: 1;
  }

  #top {
    top: 5px;
    /* transform: rotateZ(-124.5deg); */
    transform: rotateZ(-137deg);
  }

  #bottom {
    /* transform: rotateZ(55deg); */
    transform: rotateZ(45.5deg);
  }

  path {
    fill: transparent;
  }

  text {
    fill: #cf6f00;
    /* transform: scale(-1, 1); */
  }

  #angel {
    height: 587px;
    top: 136px;
  }
</style>

<section id="outer">
  <div id="inner"></div>

  <svg id="top" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" height="832px" width="832px">
  	<path id="myTextPath" d="M 350,0 A 350,350 0 0 1 -350,0 A 350,350 0 0 1 350,0" transform="translate(416,416)" fill="none" />

  	<text style="stroke: #000000;" fill="white" stroke-width="0">
      <textPath xlink:href="#myTextPath">
        <tspan dy="14">Ash angel</tspan>
      </textPath>
    </text>
  </svg>

  <svg id="bottom" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" height="832px" width="832px">
  	<path id="myTextPath" d="M 350,0 A 350,350 0 0 1 -350,0 A 350,350 0 0 1 350,0" transform="translate(416,416)" fill="none" />

  	<text style="stroke: #000000;" fill="white" stroke-width="0">
      <textPath xlink:href="#myTextPath">
        <tspan dy="14">Squadron</tspan>
      </textPath>
    </text>
  </svg>

  <!-- <img src="Ash Angel.png"> -->
  <img id="angel" src="fwpatch transparent.png">
</section>
